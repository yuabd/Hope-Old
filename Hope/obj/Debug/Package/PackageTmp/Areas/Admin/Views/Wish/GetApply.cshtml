@model Hope.Models.Apply
@{
    ViewBag.Title = "GetApply";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>GetApply</h2>
<div class="section">
	<div class="section-header">
		<h3>愿望信息</h3>
	</div>
	<div class="section-body">
		@Model.
	</div>
</div>
<div class="section">
	<div class="section-header">
		<h3>认领人信息</h3>
	</div>
	<div class="section-body">
		<div class="row">
			<label>姓名：</label>
			@Model.ContactName
		</div>
		<div class="row">
			<label>地址：</label>
			@Model.ContactAddress
		</div>
		<div class="row">
			<label>邮箱：</label>
			@Model.ContactEmail
		</div>
		<div class="row">
			<label>联系电话：</label>
			@Model.ContactTel
		</div>
		<div class="row">
			<label>留言：</label>
			@Model.Message
		</div>
	</div>
</div>